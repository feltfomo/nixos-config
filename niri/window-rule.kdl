window-rule {
  geometry-corner-radius 12
  clip-to-geometry true
  draw-border-with-background false
  blur {
    on
  }
  opacity 0.7
}
window-rule {
  match app-id="com.gabm.satty"
  open-floating true
}
window-rule {
  match app-id="kitty"
  default-column-width {}
}
window-rule {
  match app-id="floorp" title="^Picture-in-Picture$"
  open-floating true
}
layer-rule {
  match namespace="walker"
  blur {
    on
  }
}